import serial

# Open the serial port (adjust 'COM3' to the port your device is connected to)
ser = serial.Serial('COM3', baudrate=9600, timeout=1)

while True:
    if ser.in_waiting > 0:  # Read a line of data from the serial port
        data = ser.readline().decode('utf-8').strip()  # Decode bytes to string
        print(f"Received: {data}")
